{
	"AWSEBDockerrunVersion": 2,
	"containerDefinitions": [{
			"name": "frontend",
			"image": "anilverma/multi-frontend",
			"hostname": "frontend",
			"essential": "false",
			"memory": 128
		},
		{
			"name": "worker",
			"image": "anilverma/multi-worker",
			"hostname": "worker",
			"essential": "false",
			"memory": 128
		},
		{
			"name": "api",
			"image": "anilverma/multi-server",
			"hostname": "api",
			"essential": "false",
			"memory": 128
		},
		{
			"name": "nginx-proxy",
			"image": "anilverma/multi-nginx",
			"essential": "true",
			"portMappings": [{
				"hostPort": 80,
				"containerPort": 80
			}],
			"links": ["frontend"],
			"memory": 64
			
		}
	]
}
